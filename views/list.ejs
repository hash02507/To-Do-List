
    <%-include("header")-%>
    <div class="box" id="heading">
        <h1> <%=titleOfList %> </h1> 
    </div>
    <div class="box">
        <%for(var i=0;i<newListItem.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%= newListItem[i]%></p>
            </div>
            <%}%>
          
       <form action="/" method="post" class="item">
       <input type="text" name="newItem" placeholder="New Item" autocomplete="off" >
       <button type="submit" name="list" value=<%= titleOfList %>>+</button>
      </form>    
    </div>
    <%-include("footer")-%>
   
  